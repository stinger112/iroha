{
  "name": "iroha-js",
  "version": "0.0.1",
  "description": "Modern JavaScript library for Iroha. https://github.com/hyperledger/iroha",
  "main": "index.js",
  "scripts": {
    "build": "node-pre-gyp build",
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm ls",
    "publish": "node-pre-gyp-github publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperledger/iroha.git"
  },
  "keywords": [
    "iroha",
    "iroha-js",
    "irohajs",
    "iroha-node",
    "iroha-nodejs",
    "iroha-javascript",
    "iroha-client",
    "iroha-lib"
  ],
  "contributors": [
    {
      "name": "Viacheslav Bikbaev",
      "email": "viacheslav@soramitsu.co.jp"
    },
    {
      "name": "Arseniy Fokin",
      "email": "fokin@soramitsu.co.jp"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hyperledger/iroha/issues"
  },
  "homepage": "https://github.com/hyperledger/iroha#readme",
  "dependencies": {
    "google-protobuf": "^3.5.0",
    "grpc": "^1.9.1",
    "node-pre-gyp": "^0.6.39"
  },
  "devDependencies": {
    "cmake-js": "^3.6.2",
    "grpc-tools": "^1.6.6",
    "node-pre-gyp-github": "^1.3.1"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "binary": {
    "module_name": "iroha_lib",
    "module_path": "lib/{node_abi}-{platform}-{arch}-{libc}/",
    "host": "https://your_module.s3-us-west-1.amazonaws.com",
    "remote_path": "node/{name}/v{version}",
    "package_name": "{node_abi}-{platform}-{arch}-{libc}.tar.gz"
  }
}
